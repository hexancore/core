{
  "name": "@hexancore/core",
  "version": "0.12.1",
  "engines": {
    "node": ">=18"
  },
  "engine-strict": true,
  "description": "Core to build hexagonal architecture designed apps",
  "author": {
    "name": "Andrzej Wasiak",
    "email": "contact@andrzejwasiak.pl",
    "url": "https://andrzejwasiak.pl"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "testutil": [
        "./lib/Test/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "default": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./testutil": {
      "import": {
        "types": "./lib/Test/index.d.ts",
        "default": "./lib/Test/index.js"
      },
      "default": {
        "types": "./lib/Test/index.d.ts",
        "default": "./lib/Test/index.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexancore/core.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/hexancore/core.git",
  "scripts": {
    "build": "nest -p tsconfig.build.json build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest --config .jestrc.json --runInBand",
    "test:clearCache": "jest --config .jestrc.json --clearCache",
    "test:unit": "jest --config .jestrc.json --runInBand  --group=unit",
    "test:watch": "jest --config .jestrc.json --runInBand --watchAll",
    "test:cov": "jest --config .jestrc.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config .jestrc.json --runInBand",
    "prepublish": "yarn lint && yarn test && rm -fr ./lib && yarn run build"
  },
  "peerDependencies": {
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cli": "^10.1.11",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/cqrs": "^10.2.5",
    "@nestjs/platform-fastify": "^10.1.3",
    "@nestjs/swagger": "^7.1.8",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "dependencies": {
    "@fastify/cookie": "9.0.4",
    "@fastify/cors": "8.3.0",
    "@fastify/middie": "8.3.0",
    "@fastify/multipart": "7.7.3",
    "@fastify/static": "^6.10.2",
    "@hexancore/common": "0.12.3",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "bull": "^4.8.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "deep-equal": "^2.0.5",
    "dotenv": "^16.0.3",
    "fastify": "4.21.0",
    "fastify-swagger": "^5.2.0",
    "file-type": "^16.4.0",
    "fs-extra": "^11.1.1",
    "ioredis": "^5.3.2",
    "js-yaml": "^4.1.0",
    "lambda-log": "^3.1.0",
    "process-warning": "^2.3.2",
    "sodium-native": "^4.0.4",
    "tslib": "^2.5.0",
    "typescript-chained-error": "^1.6.0"
  },
  "devDependencies": {
    "@hexancore/mocker": "^1.1.2",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cli": "^10.1.11",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/cqrs": "^10.2.5",
    "@nestjs/platform-fastify": "^10.1.3",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/testing": "^10.1.3",
    "@types/benchmark": "^2.1.4",
    "@types/jest": "27.0.*",
    "@types/js-yaml": "^4.0.5",
    "@types/lambda-log": "^3.0.0",
    "@types/node": "^18.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "axios": "^1.5.1",
    "axios-mock-adapter": "^1.22.0",
    "benchmark": "^2.1.4",
    "eslint": "^8.3.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "29.6.*",
    "jest-docblock": "29.4.*",
    "jest-expect-message": "^1.1.3",
    "jest-runner": "29.6.*",
    "jest-runner-groups": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "string.prototype.matchall": "^4.0.10",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.*",
    "ts-node": "^10.9.1",
    "ts-paths-esm-loader": "^1.4.3",
    "tsx": "^3.14.0",
    "typescript": "5.2.2"
  },
  "bugs": {
    "url": "https://github.com/hexancore/hexancore/issues"
  },
  "packageManager": "yarn@4.0.2"
}
